function calculateResult(a,b,c,d,e,f){
  let sum = a + b;
  let difference = sum - c;
  let multiply = difference * d / e;
  let power = Math.pow(multiply, f);
  return power;
}

calculateResult(6,5,4,3,2,1);
calculateResult(6,2,1,4,2,3);
calculateResult(2,3,6,4,2,3);

function singleLine(a,b,c,d,e,f){
  //return Math.pow(((a+b-c)*d/e),f);
  return ((a+b-c)*d/e)**f;
}

singleLine(6,5,4,3,2,1);
singleLine(6,2,1,4,2,3);
singleLine(2,3,6,4,2,3);




